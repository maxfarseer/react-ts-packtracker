    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.24.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^4.2.0",
    "generate-json-webpack-plugin": "^0.3.1",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.8",
    "jest": "^27.0.6",
    "json-minify": "^1.0.0",
    "lint-staged": "^9.4.1",
    "mini-css-extract-plugin": "^0.8.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "pascal-case": "^2.0.1",
    "prettier": "^1.18.2",
    "react-hot-loader": "^4.13.0",
    "react-is": "^16.13.1",
    "style-loader": "^1.3.0",
    "ts-jest": "^27.0.4",
    "ts-loader": "^8.3.0",
    "typesafe-actions": "^4.4.2",
    "typescript": "^4.3.5",
    "webpack": "^4.46.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@shore/i18n": "1.2.0",
    "history": "^4.10.1",
    "i18n": "^0.8.6",
    "i18next-client": "1.11.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md}": [
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shore-gmbh/react-ts-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/shore-gmbh/react-ts-boilerplate/issues"
  },
  "homepage": "https://github.com/shore-gmbh/react-ts-boilerplate#readme"
}
