{
  "name": "react-ts-boilerplate",
  "version": "1.0.0",
  "description": "Shore React-Typescript boilerplate",
  "author": "Shore Tech <tech@shore.com>",
  "main": "index.js",
  "scripts": {
    "start": "./config/scripts/dev.sh",
    "dev": "./config/scripts/dev.sh",
    "build": "./config/scripts/build.sh",
    "build:validate": "CDN_BASE_URL=//dummy.cdn/ npm run build",
    "lint": "eslint 'src/**/*.ts?(x)'",
    "lint:fix": "npm run lint --fix",
    "clear:jest": "jest --clearCache",
    "test": "jest --config=./jest.config.js --passWithNoTests",
    "test:watch": "jest --config=./jest.config.js --watch",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "check-outdated": "npm outdated"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.15.0",
    "@hot-loader/react-dom": "^17.0.1",
    "@shore/types": "^1.1.7",
    "@storybook/addon-actions": "^6.3.7",
    "@storybook/addon-controls": "^6.3.7",
    "@storybook/addon-essentials": "^6.3.7",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/client-api": "^6.3.7",
    "@storybook/react": "^6.3.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/moment-timezone": "^0.5.30",
    "@types/react": "^17.0.18",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/styled-components": "^5.1.12",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.16.2",
    "@types/webpack-merge": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.29.1",
    "@typescript-eslint/parser": "^4.29.1",
    "babel-loader": "^8.2.2",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.16.1",
    "css-loader": "^5.2.7",
    "dotenv": "^10.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
